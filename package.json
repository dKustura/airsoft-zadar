  "scripts": {
    "start": "npm run trans && react-scripts start",
    "build": "npm run trans && react-scripts build",
    "test": "npm run trans && react-scripts test",
    "eject": "react-scripts eject",
    "trans:extract": "extract-messages -l=en -o src/translations/locales -d hr --flat true src/**/messages.ts",
